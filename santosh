pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        
    stage('build'){
        steps {
            echo 'this is the build process'
        }
    }
    stage('deploy'){
        steps {
            echo 'this is deploy'
        }
    }
        
    
        
               
            
        
    }
}
